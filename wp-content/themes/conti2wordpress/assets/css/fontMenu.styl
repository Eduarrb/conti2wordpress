@font-face {
    font-family: 'fontMenu';
    src:    url('../fonts/fontMenu.eot?8unyec');
    src:    url('../fonts/fontMenu.eot?8unyec#iefix') format('embedded-opentype'),
        url('../fonts/fontMenu.ttf?8unyec') format('truetype'),
        url('../fonts/fontMenu.woff?8unyec') format('woff'),
        url('../fonts/fontMenu.svg?8unyec#fontMenu') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'fontMenu' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-menu:before {
    content: "\e900";
}

